ADMIN_SETTING_PASSWORD=

ALIBABA_API_KEY=

# 覆盖API请求基础URL（可选）
# 默认: https://dashscope.aliyuncs.com/compatible-mode/v1
# 示例: http://your-api-proxy.com
BASE_URL=https://dashscope.aliyuncs.com/compatible-mode/v1

# (optional)
# Default: Empty
# Change default model
DEFAULT_MODEL=qwen-turbo-latest

# (optional)
# Default: Empty
# Change default provider
# 可用值: Alibaba, Azure, Google, Anthropic, Baidu, ByteDance, Tencent, Moonshot, Iflytek, DeepSeek, XAI, ChatGLM, SiliconFlow
# 注意: OpenAI已被禁用
DEFAULT_PROVIDER=Alibaba

# 数据库配置
DATABASE_URL=

# MCP功能开关
ENABLE_MCP=true

# 前端客户端 API 配置（浏览器端可访问）
# 必须以 NEXT_PUBLIC_ 开头才能在客户端使用
# 默认: http://localhost:5000
# 生产环境示例: https://your-server-ip 或 https://your-domain.com
# 注意: 此配置同时用于控制前端日志输出
#       包含 localhost 或 127.0.0.1 时为开发环境，日志正常输出
#       其他域名时为生产环境，日志将被禁用以提高安全性
NEXT_PUBLIC_API_BASE_URL=http://localhost:5000
